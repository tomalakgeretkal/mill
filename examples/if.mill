use std::io;

MAIN {
    if true {
        io::writeln("1: OK");
    } else {
        io::writeln("1: WRONG");
    };

    if false {
        io::writeln("2: WRONG");
    } else {
        io::writeln("2: OK");
    };

    if true {
        io::writeln("3: OK");
    };

    if false {
        io::writeln("4: WRONG");
    };
}
